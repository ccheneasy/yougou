

.searchBox{
  box-sizing: border-box;
  width: 750rpx;
  background-color: #eeeeee;
  padding: 30rpx 20rpx;
}
.input{
  box-sizing: border-box;
  width: 100%;
  padding: 10rpx;
  background-color: #fff;
  font-size: 12px;
  display: flex;
  align-items: center
}

.iconweibiaoti7{
  font-size: 14px;
}

.goodsListTitle{
  display: flex;
  justify-content: space-around;
  font-size: 14px;
  padding: 35rpx;
  border-bottom: 1px solid #f5f5f5
}

.goodsListItem{
  display: flex;
  padding: 20rpx;
  border-bottom: 1px solid #eee
}

.itemImage{
  margin-right: 20rpx;
  width: 200rpx;
  height: 200rpx;
}


.itemDetail{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between
}
.detailText{
  font-size:14px; 
  height: 38px;

  /* 多行文本省略显示 */
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp:2;
  display:-webkit-box; 
  -webkit-box-orient:vertical; 
}

.itemDetail text{
  color: #eb4652;
  font-size: 18px
}

.loading{
  padding: 30rpx 0;
  text-align: center;
  font-size: 12px;
  color: #666
}