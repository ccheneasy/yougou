
.container{
  padding-bottom: 100rpx;
  width: 750rpx;
}
.getinfo{
  margin-left: 20rpx;
  margin-top: 20rpx;
  font-size: 14px;
}
.info{
  box-sizing: border-box;
  padding: 20rpx;
  font-size: 14px;
  display: flex;
  justify-content: space-between;
  align-items: center
}

.men{
  margin: 10rpx 0;
}

.tel{
  display: flex;
  justify-content: space-between;
  align-items: center
}

.tel view{
  margin-right: 40rpx
}

.adress{
  font-size: 14px;
  margin-left: 20rpx;
}
.img{
  height: 10rpx;
  width: 100%
}

.content{
  box-sizing: border-box;
  padding: 10rpx 0 10rpx 30rpx;
}
.title{
  display: flex;
  align-items: center;
  font-size: 14px;
  margin: 20rpx 0
}
.icondianpu{
  margin-right: 10rpx
}


.contentitem{
  display: flex;
  align-items: center;
  justify-content: space-between
}

.contentitem icon{
  margin-right: 30rpx
}
.itemcontent{
  height: 100%;
  border-top: 1px solid #eee ;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 0
}

.itemcontent image{
  width: 160rpx;
  height: 160rpx;
  flex-shrink: 0
}

.itemdetail{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin: 0 20rpx;
}

.itemdetailtitle{
  font-size: 12px;
  margin-bottom: 50rpx;
  overflow:hidden;
  display:-webkit-box; 
  -webkit-line-clamp:2; 
  -webkit-box-orient:vertical; 
}
.price{
  color: #ff2d4a;
  display: flex;
  justify-content: space-between;
  align-items: center
}

.price view{
  font-size: 14px;
  color: #666;
  margin-right: 20rpx;
}

.runpay{
  position: fixed;
  display: flex;
  bottom: 0;
  width: 100%;
  align-items: center;
  background-color: #fff
}
.runpay view{
  font-size: 14px;
  margin-left:20rpx; 
}
.runpay button{
  border-radius: 8px
}