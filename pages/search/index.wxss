

.searchBox{
  width: 750rpx;
  height: 80rpx;
  background-color: #eeeeee;
  padding: 30rpx 20rpx;
  display: flex;
  justify-content: center;
  align-items: center
}
.input{
  flex: 1;
  padding: 10rpx;
  background-color: #fff;
  font-size: 12px;
  display: flex;
  align-items: center
}
.button{
  width: 100rpx;
  margin: 0 40rpx;
  padding: 5rpx 20rpx;
  border: 1px solid #d7d6d7;
  text-align: center;
  border-radius: 10rpx;
  font-size: 14px
}


.historyTitle{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 25rpx 20rpx;
}

.historyContent{
  display: flex;
  flex-wrap:wrap; 
}
.historyItem{
  padding: 20rpx;
  margin: 10rpx 20rpx;
  background-color: #dddddd;
  font-size: 12px;
  color: #555;
  
}


.goodsListTitle{
  display: flex;
  justify-content: space-around;
  font-size: 14px;
  padding: 35rpx;
  border-bottom: 1px solid #f5f5f5
}

.goodsListItem{
  display: flex;
  padding: 20rpx;
}

.itemImage{
  width: 250rpx;
  height: 250rpx;
  background-color: skyblue;
  margin-right: 20rpx;
}
.itemDetail{
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between
}
.detailText{
  font-size:14px; 
  height: 38px;

  /* 多行文本省略显示 */
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp:2;
  display:-webkit-box; 
  -webkit-box-orient:vertical; 
}

.itemDetail text{
  color: #eb4652;
  font-size: 18px
}